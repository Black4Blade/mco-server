{
  "name": "mco-server",
  "version": "1.2.0",
  "description": "a server",
  "main": "server.js",
  "scripts": {
    "dev": "npm install && npm run tslint && npm build -w",
    "build": "npm run build-ts && npm run tslint",
    "start": "node server.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "pretest": "npm run eslint",
    "test": "npm run build && jest --all",
    "build-ts": "tsc",
    "licence-check": "license-checker --json > ./data/licenses.json"
  },
  "author": "Joseph W Becher <jwbecher@drazisil.com> (drazisil)",
  "license": "GPL-3.0+",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:drazisil/mco-server.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "lib/**/*.js"
    ],
    "testResultsProcessor": "jest-junit",
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": ".*(spec|test).(ts|tsx|js)$"
  },
  "jest-junit": {
    "output": "test-reports/junit.xml"
  },
  "dependencies": {
    "arc4": "^3.4.0",
    "async": "^2.5.0",
    "dotenv": "^5.0.0",
    "require-environment-variables": "^1.1.2",
    "sqlite": "^2.9.1",
    "ssl-config": "^1.0.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "jest": "^22.4.0",
    "jest-junit": "^3.6.0",
    "license-checker": "^18.0.0",
    "pg-migrator": "^1.0.5",
    "prettier": "^1.12.0",
    "prettier-eslint": "^8.8.1",
    "prettifier": "^0.3.5"
  },
  "prettier": {
    "trailingComma": "es5"
  }
}
